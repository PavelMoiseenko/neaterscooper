#addressSection input[type="submit"] {
    background-color: #ec000e;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#ec000e), to(#c9000a));
    background-image: -webkit-linear-gradient(top, #ec000e, #c9000a);
    background-image: -moz-linear-gradient(top, #ec000e, #c9000a);
    background-image: -o-linear-gradient(top, #ec000e, #c9000a);
    background-image: -ms-linear-gradient(top, #ec000e, #c9000a);
    background-image: linear-gradient(top, #ec000e, #c9000a);
    filter: progid: DXImageTransform.Microsoft.gradient(GradientType=0, StartColorStr='#ec000e', EndColorStr='#c9000a');
